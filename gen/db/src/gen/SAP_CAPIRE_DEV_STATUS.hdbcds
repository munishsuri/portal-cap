using SAP_CAPIRE_DEV_STATUS_TEXTS as SAP_CAPIRE_DEV_STATUS_TEXTS;
entity SAP_CAPIRE_DEV_STATUS {
  DESCR : String(1000);
  NAME : String(5000);
  key CODE : String(5000) default 'OPEN';
  TEXTS : association[*] to SAP_CAPIRE_DEV_STATUS_TEXTS on TEXTS.CODE = CODE;
  LOCALIZED : association to SAP_CAPIRE_DEV_STATUS_TEXTS on LOCALIZED.CODE = CODE and LOCALIZED.LOCALE = SESSION_CONTEXT('LOCALE');
};
