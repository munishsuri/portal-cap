using SAP_CAPIRE_DEV_INDIVIDUAL_TEXTS as SAP_CAPIRE_DEV_INDIVIDUAL_TEXTS;
entity SAP_CAPIRE_DEV_INDIVIDUAL {
  DESCR : String(1000);
  key CODE : String(5000);
  NAME : String(5000);
  TEXTS : association[*] to SAP_CAPIRE_DEV_INDIVIDUAL_TEXTS on TEXTS.CODE = CODE;
  LOCALIZED : association to SAP_CAPIRE_DEV_INDIVIDUAL_TEXTS on LOCALIZED.CODE = CODE and LOCALIZED.LOCALE = SESSION_CONTEXT('LOCALE');
};
