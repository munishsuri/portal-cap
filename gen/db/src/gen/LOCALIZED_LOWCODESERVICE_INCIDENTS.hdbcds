using LOCALIZED_SAP_CAPIRE_DEV_INCIDENTS as LOCALIZED_SAP_CAPIRE_DEV_INCIDENTS;
using LOCALIZED_LOWCODESERVICE_STATUS as LOCALIZED_LOWCODESERVICE_STATUS;
using LOCALIZED_LOWCODESERVICE_CATEGORY as LOCALIZED_LOWCODESERVICE_CATEGORY;
using LOCALIZED_LOWCODESERVICE_PRIORITY as LOCALIZED_LOWCODESERVICE_PRIORITY;
using LOCALIZED_LOWCODESERVICE_INDIVIDUAL as LOCALIZED_LOWCODESERVICE_INDIVIDUAL;
view LOCALIZED_LOWCODESERVICE_INCIDENTS as select from LOCALIZED_SAP_CAPIRE_DEV_INCIDENTS as INCIDENTS mixin {
  __STATUS : association to LOCALIZED_LOWCODESERVICE_STATUS on __STATUS.CODE = $projection.STATUS_CODE;
  __CATEGORY : association to LOCALIZED_LOWCODESERVICE_CATEGORY on __CATEGORY.CODE = $projection.CATEGORY_CODE;
  __PRIORITY : association to LOCALIZED_LOWCODESERVICE_PRIORITY on __PRIORITY.CODE = $projection.PRIORITY_CODE;
  __ASSIGNEDINDIVIDUAL : association[1] to LOCALIZED_LOWCODESERVICE_INDIVIDUAL on __ASSIGNEDINDIVIDUAL.CODE = $projection.ASSIGNEDINDIVIDUAL_CODE;
} into {
  key INCIDENTS.ID as ID,
  INCIDENTS.TITLE,
  INCIDENTS.DESCRIPTION,
  INCIDENTS.LTI,
  __STATUS as STATUS,
  INCIDENTS.LOCATION,
  __CATEGORY as CATEGORY,
  __PRIORITY as PRIORITY,
  INCIDENTS.CLOSED_ON,
  __ASSIGNEDINDIVIDUAL as ASSIGNEDINDIVIDUAL,
  INCIDENTS.STATUS_CODE,
  INCIDENTS.CATEGORY_CODE,
  INCIDENTS.PRIORITY_CODE,
  INCIDENTS.ASSIGNEDINDIVIDUAL_CODE
};
