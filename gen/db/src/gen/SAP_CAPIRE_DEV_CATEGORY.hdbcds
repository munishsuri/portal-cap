using SAP_CAPIRE_DEV_CATEGORY_TEXTS as SAP_CAPIRE_DEV_CATEGORY_TEXTS;
entity SAP_CAPIRE_DEV_CATEGORY {
  DESCR : String(1000);
  key CODE : String(20);
  NAME : String(5000);
  TEXTS : association[*] to SAP_CAPIRE_DEV_CATEGORY_TEXTS on TEXTS.CODE = CODE;
  LOCALIZED : association to SAP_CAPIRE_DEV_CATEGORY_TEXTS on LOCALIZED.CODE = CODE and LOCALIZED.LOCALE = SESSION_CONTEXT('LOCALE');
};
