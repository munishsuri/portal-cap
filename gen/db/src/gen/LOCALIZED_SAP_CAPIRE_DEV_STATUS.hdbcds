using SAP_CAPIRE_DEV_STATUS as SAP_CAPIRE_DEV_STATUS;
using SAP_CAPIRE_DEV_STATUS_TEXTS as SAP_CAPIRE_DEV_STATUS_TEXTS;
view LOCALIZED_SAP_CAPIRE_DEV_STATUS as select from (SAP_CAPIRE_DEV_STATUS as L_0 left join SAP_CAPIRE_DEV_STATUS_TEXTS as LOCALIZED_1 on LOCALIZED_1.CODE = L_0.CODE and LOCALIZED_1.LOCALE = SESSION_CONTEXT('LOCALE')) mixin {
  ___TEXTS : association[*] to SAP_CAPIRE_DEV_STATUS_TEXTS on ___TEXTS.CODE = $projection.CODE;
  ___LOCALIZED : association to SAP_CAPIRE_DEV_STATUS_TEXTS on ___LOCALIZED.CODE = $projection.CODE and ___LOCALIZED.LOCALE = SESSION_CONTEXT('LOCALE');
} into {
  coalesce(LOCALIZED_1.DESCR, L_0.DESCR) as DESCR,
  L_0.NAME,
  key L_0.CODE as CODE,
  ___TEXTS as TEXTS,
  ___LOCALIZED as LOCALIZED
};
