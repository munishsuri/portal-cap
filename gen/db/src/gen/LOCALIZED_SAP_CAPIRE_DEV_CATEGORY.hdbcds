using SAP_CAPIRE_DEV_CATEGORY as SAP_CAPIRE_DEV_CATEGORY;
using SAP_CAPIRE_DEV_CATEGORY_TEXTS as SAP_CAPIRE_DEV_CATEGORY_TEXTS;
view LOCALIZED_SAP_CAPIRE_DEV_CATEGORY as select from (SAP_CAPIRE_DEV_CATEGORY as L_0 left join SAP_CAPIRE_DEV_CATEGORY_TEXTS as LOCALIZED_1 on LOCALIZED_1.CODE = L_0.CODE and LOCALIZED_1.LOCALE = SESSION_CONTEXT('LOCALE')) mixin {
  ___TEXTS : association[*] to SAP_CAPIRE_DEV_CATEGORY_TEXTS on ___TEXTS.CODE = $projection.CODE;
  ___LOCALIZED : association to SAP_CAPIRE_DEV_CATEGORY_TEXTS on ___LOCALIZED.CODE = $projection.CODE and ___LOCALIZED.LOCALE = SESSION_CONTEXT('LOCALE');
} into {
  coalesce(LOCALIZED_1.DESCR, L_0.DESCR) as DESCR,
  key L_0.CODE as CODE,
  L_0.NAME,
  ___TEXTS as TEXTS,
  ___LOCALIZED as LOCALIZED
};
